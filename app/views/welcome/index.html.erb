<div class="container">

<div class="row">
		<div class="col-md-6">
			<div class="page-header">
			  <h2>Tenants List</h2>
			</div>

	<table class="table">
	<thead>
		<tr>
			<th>Users</th>
			<th>Questions</th>
			<th>Answers</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td><%=@users.count%></td>
			<td><%=@questions.count%></td>
			<td><%=@answer.count%></td>
		</tr>
	</tbody>
</table>
	
</div>
<div class="col-md-6">
<div class="page-header">
  <h2>Tenants List</h2>
</div>
<table class="table table-hover">
	<thead>
		<tr>
			<th>Name</th>
			<th>counts</th>
			
		</tr>
	</thead>
	<tbody>
		<%@tenants.each do |tenant|%>
		<tr>
			
			<td><%=tenant.name%></td>
			<td><%=tenant.count%></td>
		</tr>
		<%end%>
	</tbody>
</table>
		
	
</div>
</div>

</div>

